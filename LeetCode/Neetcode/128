class Solution:
    def longestConsecutive(self, nums):
        s = set(nums)
        longest = 0
        for x in s:
            if x - 1 not in s:
                curr = x
                length = 1
                while curr + 1 in s:
                    curr += 1
                    length += 1
                longest = max(longest, length)
        return longest
